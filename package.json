{
  "name": "ear-monster",
  "packageManager": "pnpm@7.3.0",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "private": false,
  "version": "1.0.0",
  "main": "umd/index.js",
  "unpkg": "umd/index.js",
  "typings": "types/index.d.ts",
  "style": "umd/style.css",
  "module": "es/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "build": "pnpm --filter=@ear-monster/* run build",
    "release": "changeset publish --access public",
    "version": "changeset version",
    "preinstall": "npx only-allow pnpm",
    "changeset": "changeset",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@changesets/git": "^2.0.0",
    "typescript": "^4.1.5",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-sass-dts": "^1.1.20",
    "vitepress": "^0.20.1"
  },
  "author": "chen bing",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": true,
      "bump": true
    }
  },
  "lint-staged": {
    "*.{js,ts,css,vue,scss}": "prettier  --plugin-search-dir ./node_modules --config .prettierrc --write"
  },
  "dependencies": {
    "vite": "^2.0.0"
  },
  "description": "一些常用的ui组件合集",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/duochan/request_cb.git"
  }
}
